[32m+[m[32m  const computeCompatibilityScore = (aAttrs, bAttrs) => {[m
[32m+[m[32m    const weights = {[m
[32m+[m[32m      digestionTimeDiff: 0.30,[m
[32m+[m[32m      viryaOpposition: 0.25,[m
[32m+[m[32m      heavinessSum: 0.20,[m
[32m+[m[32m      doshaConflict: 0.15,[m
[32m+[m[32m      specialPatterns: 0.10[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    let score = 100;[m
[32m+[m[32m    const breakdown = [];[m
[32m+[m
[32m+[m[32m    const dtDiff = Math.abs((aAttrs.digestionTime || 90) - (bAttrs.digestionTime || 90));[m
[32m+[m[32m    const dtPenalty = Math.min(1, dtDiff / 240) * 100 * weights.digestionTimeDiff;[m
[32m+[m[32m    score -= dtPenalty;[m
[32m+[m[32m    breakdown.push({ factor: 'digestionTime', weight: -dtPenalty, explanation: `Digestion times differ by ${dtDiff} minutes` });[m
[32m+[m
[32m+[m[32m    const viryaOpposition = (aAttrs.virya !== bAttrs.virya) && (aAttrs.virya !== 'neutral' && bAttrs.virya !== 'neutral');[m
[32m+[m[32m    if (viryaOpposition) {[m
[32m+[m[32m      const vPenalty = 100 * weights.viryaOpposition;[m
[32m+[m[32m      score -= vPenalty;[m
[32m+[m[32m      breakdown.push({ factor: 'viryaOpposition', weight: -vPenalty, explanation: `${aAttrs.virya} vs ${bAttrs.virya} (opposing thermal effects)` });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      breakdown.push({ factor: 'viryaOpposition', weight: 0, explanation: 'No major heating/cooling opposition' });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const heavinessSum = (aAttrs.heaviness || 2) + (bAttrs.heaviness || 2);[m
[32m+[m[32m    const heavinessPenalty = Math.max(0, (heavinessSum - 6) / 8) * 100 * weights.heavinessSum;[m
[32m+[m[32m    score -= heavinessPenalty;[m
[32m+[m[32m    breakdown.push({ factor: 'heaviness', weight: -heavinessPenalty, explanation: `Combined heaviness score ${heavinessSum}` });[m
[32m+[m
[32m+[m[32m    let doshaPenalty = 0;[m
[32m+[m[32m    if (aAttrs.dominantDosha === 'kapha' && bAttrs.dominantDosha === 'kapha') {[m
[32m+[m[32m      doshaPenalty = 100 * weights.doshaConflict * 0.9;[m
[32m+[m[32m      breakdown.push({ factor: 'doshaConflict', weight: -doshaPenalty, explanation: 'Both foods increase kapha â€” risk of heaviness/ama' });[m
[32m+[m[32m    } else if (aAttrs.dominantDosha === bAttrs.dominantDosha) {[m
[32m+[m[32m      doshaPenalty = 100 * weights.doshaConflict * 0.4;[m
[32m+[m[32m      breakdown.push({ factor: 'doshaSimilarity', weight: -doshaPenalty, explanation: `Both foods lean ${aAttrs.dominantDosha}` });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      breakdown.push({ factor: 'dosha', weight: 0, explanation: 'No major dosha conflict' });[m
[32m+[m[32m    }[m
[32m+[m[32m    score -= doshaPenalty;[m
[32m+[m
[32m+[m[32m    // special pattern: honey + ghee -> small bonus[m
[32m+[m[32m    let specialAdjustment = 0;[m
[32m+[m[32m    const pair = `${aAttrs._value || ''}-${bAttrs._value || ''}`;[m
[32m+[m[32m    if ((pair === 'honey-ghee') || (pair === 'ghee-honey')) {[m
[32m+[m[32m      specialAdjustment = 10;[m
[32m+[m[32m      score += specialAdjustment;[m
[32m+[m[32m      breakdown.push({ factor: 'special', weight: +specialAdjustment, explanation: 'Honey + Ghee (in proper proportion) gives a traditional benefit' });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    score = Math.max(0, Math.min(100, Math.round(score)));[m
[32m+[m[32m    return { score, breakdown, specialAdjustment };[m
[32m+[m[32m  };[m
